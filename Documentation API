  <span class="token string">"total"</span><span class="token punctuation">:</span> <span class="token number">14.504</span><span class="token punctuation">,</span>	<span class="token comment" spellcheck="true">//montant TTC total de l'order</span>
  <span class="token string">"content"</span><span class="token punctuation">:</span> <span class="token punctuation">[</span>	<span class="token comment" spellcheck="true">//liste des items de l'order</span>
    <span class="token punctuation">{</span>
      <span class="token string">"externalId"</span><span class="token punctuation">:</span> <span class="token keyword">null</span><span class="token punctuation">,</span>	<span class="token comment" spellcheck="true">//external id de l'item</span>
