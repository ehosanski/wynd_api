  <span class="token string">"picture_url"</span><span class="token punctuation">:</span> <span class="token string">"https:// [...] /petit_dejeuner.jpg"</span><span class="token punctuation">,</span>
  <span class="token string">"type"</span><span class="token punctuation">:</span> <span class="token punctuation">{</span>
